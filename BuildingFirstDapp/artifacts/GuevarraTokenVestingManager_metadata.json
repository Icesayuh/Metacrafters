					"params": {
						"_name": "The name of the stakeholder type",
						"_vestingPeriod": "The vesting period in seconds"
					}
				},
				"claimTokens(address)": {
					"details": "Claim vested tokens",
					"params": {
						"_orgAddress": "The address of the organization"
					}
				},
				"createVestingSchedule(address,bytes32,uint256)": {
					"details": "Create a vesting schedule for a beneficiary",
					"params": {
						"_amount": "The amount of tokens to vest",
						"_beneficiary": "The address of the beneficiary",
						"_stakeholderTypeId": "The ID of the stakeholder type"
					}
				},
				"getVestingInfo(address,address)": {
					"details": "Get vesting information for a beneficiary",
					"params": {
						"_beneficiary": "The address of the beneficiary",
						"_orgAddress": "The address of the organization"
					},
					"returns": {
						"amount": "The total amount of tokens in the vesting schedule",
						"claimedAmount": "The amount of tokens already claimed",
						"stakeholderTypeId": "The ID of the stakeholder type",
						"startTime": "The start time of the vesting schedule"
					}
				},
				"registerOrganization(address)": {
					"details": "Register a new organization and its token",
					"params": {
						"_tokenAddress": "The address of the organization's ERC20 token"
					}
